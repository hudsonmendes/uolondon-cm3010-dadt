{
    "configurations": [
        {
            "name": "web",
            "program": "${workspaceFolder}/web/index.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "name": "etl",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/etl",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--pdd_max_rows_each_file", "100",
                "--ofsted_max_rows_each_file", "1000"
            ]
        },
    ]
}